<h1>Listing initial_projects</h1>

<table>
  <tr>
    <th>User</th>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @initial_projects.each do |initial_project| %>
  <tr>
    <td><%= initial_project.user_id %></td>
    <td><%= initial_project.name %></td>
    <td><%= link_to 'Show', initial_project %></td>
    <td><%= link_to 'Edit', edit_initial_project_path(initial_project) %></td>
    <td><%= link_to 'Destroy', initial_project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

